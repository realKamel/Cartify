{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext", 
      "WithThreadId",  
      "WithProcessId" ,
      "WithMachineName",
      "WithCorrelationId",
      "WithRequestPath",
      "WithExceptionDetails"
    ]
  }
}
