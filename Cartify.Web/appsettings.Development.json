{
	"Serilog": {
		"Using": [
			"Serilog.Sinks.Console"
		],
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"Microsoft": "Information",
				"System": "Information"
			}
		},
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}] [{SourceContext}] {Message:lj}{NewLine}{Exception}"
				}
			}
		],
		"Enrich": [
			"FromLogContext",
			"WithThreadId",
			"WithProcessId",
			"WithMachineName",
			"WithCorrelationId",
			"WithRequestPath",
			"WithExceptionDetails"
		]
	},
	"ConnectionStrings": {
		"DefaultConnection": "",
		"IdentityConnection": "",
		"RedisConnection": ""
	},
	"JwtConfigurationOptions": {
		"Issuer": "https://localhost:5193",
		"Audience": "https://localhost:5193/api",
		"RefreshTokenExpirationInDays": 7,
		"AccessTokenExpirationInMinutes": 15,
		"SecurityKey": ""
	}
}
