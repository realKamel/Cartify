{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithEnvironmentName",
      "WithCorrelationId"
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "",
    "IdentityConnection": ""
  },
  "JwtConfigurationOptions": {
    "Issuer": "",
    "Audience": "",
    "RefreshTokenExpirationInDays": 7,
    "AccessTokenExpirationInMinutes": 15,
    "SecurityKey": ""
  },
  "AllowedHosts": "*"
}
